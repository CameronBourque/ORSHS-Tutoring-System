<div class="container">
  <div class="row">
    <div class="col-12">
      <h1>New Department Page</h1>
      <%= form_for :depatrments  url: create_departemnts_path do |f| %>

        <div class="field">
          <%= f.label :department_name %>
          <%= f.text_field :department_name,  autocomplete: "CSCE", class: "form-control", required: true %>
        </div>

        <div class="actions">
          <%= f.submit "Create", class: 'btn btn-primary mt-2' %>
        </div>

      <% end %>

      <%= link_to "Back", :back %>
    </div>
  </div>
</div>
