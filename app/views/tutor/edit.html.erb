
<div class="container">
  <div class="row">
    <div class="col-12">
      <h1>Edit Tutor Courses Page</h1>
      <%= form_for(edit_tutor_path(current_user)) do |f| %>

      <div class="field">
        <% Courses.all.each do |c| %>
          <%= c.course_name %>
          <br />
        <% end %>
      </div>

      <div class="actions">
        <%= f.submit "Update", class: 'btn btn-primary mt-2' %>
      </div>

      <%end%>
    </div>
  </div>
</div>
